<!DOCTYPE html>
<html>
<head>
  <title>Science</title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>  
  <%= javascript_include_tag :defaults %> 
</head>
<body>

<div class="container">

  <%= render 'layouts/header' %>
    <% flash.each do |key, value| %>
      <div class="flash <%= key %>"> <%= value %> </div>
    <% end %>
  <%= yield %>
  <%= debug(params) if Rails.env.development? %> 
  <%= render 'layouts/footer' %>
</div>
</body>
</html>
